<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="832.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.formalspeech.server.fx.windows.mainWindow.createUserPane.CreateUserPaneController">
   <children>
      <HBox alignment="CENTER" prefWidth="832.0">
         <children>
            <VBox prefHeight="600.0" prefWidth="420.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="192.0">
                     <children>
                        <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../account.png" />
                           </image>
                        </ImageView>
                     </children>
                  </HBox>
                  <ScrollPane>
                     <content>
                        <VBox fx:id="vbUserInfo" prefHeight="200.0" prefWidth="400.0" />
                     </content>
                  </ScrollPane>
                  <Pane prefHeight="40.0" prefWidth="420.0">
                     <children>
                        <HBox alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="420.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#onCancelClicked" text="Cancel">
                                 <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </HBox.margin>
                              </Button>
                              <Button mnemonicParsing="false" onAction="#onCreateClicked" text="Create">
                                 <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </HBox.margin>
                              </Button>
                           </children>
                        </HBox>
                     </children>
                  </Pane>
               </children></VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
